<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "Net Price Compare", root_path, id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%#= link_to "Item Search",  new_item_path  %></li>
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Item Search <b class="caret"></b>
            </a>
              <ul class="dropdown-menu">
              <li><%= link_to "Rakuten", new_item_path %></li>
              <li><%= link_to "Amazon", new_amitem_path %></li>
              </ul>
        </li> 
      
        <li><%#= link_to "Item Ranking",   '#' %></li>
           <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Item Ranking <b class="caret"></b>
            </a>
              <ul class="dropdown-menu">
              <li><%= link_to "Toy", '#' %></li>
              <li><%= link_to "Book", '#' %></li>
              </ul>
           </li>  
              
        <li><%= link_to "Carrier Tracking",   '#' %></li>
        
        <% if logged_in? %>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Account <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "Profile", current_user %></li>
              <li><%= link_to "Settings", edit_user_path(current_user) %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "Log out", logout_path, method: "delete" %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>